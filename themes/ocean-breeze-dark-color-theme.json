{
  "$schema": "vscode://schemas/color-theme",
  "name": "Ocean Breeze Dark",
  "displayName": "Ocean Breeze Dark",
  "description": "A dark theme with ocean-inspired colors featuring teal, warm orange, and soft purple accents",
  "author": "Martin Matanovic",
  "type": "dark",
  "semanticHighlighting": true,
  
  "semanticTokenColors": {
    // Enhanced semantic token support for modern languages
    "enumMember": {
      "foreground": "#40e0d0"
    },
    "variable.constant": {
      "foreground": "#5cc9f5"
    },
    "variable.defaultLibrary": {
      "foreground": "#ffa07a"
    },
    "class": {
      "foreground": "#ffa07a"
    },
    "interface": {
      "foreground": "#40e0d0"
    },
    "namespace": {
      "foreground": "#dda0dd"
    },
    "type": {
      "foreground": "#ffa07a"
    },
    "parameter": {
      "foreground": "#e0e6ed"
    },
    "property": {
      "foreground": "#ffa07a"
    },
    "method": {
      "foreground": "#dda0dd"
    },
    "function": {
      "foreground": "#dda0dd"
    },
    "keyword": {
      "foreground": "#40e0d0"
    },
    "decorator": {
      "foreground": "#dda0dd"
    },
    "macro": {
      "foreground": "#ffa07a"
    }
  },
  
  "tokenColors": [
    // Base language elements
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8a9ba8"
      }
    },
    {
      "name": "Documentation Comments",
      "scope": [
        "comment.block.documentation",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#40e0d0"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.logical",
        "keyword.operator.bitwise",
        "keyword.operator.channel"
      ],
      "settings": {
        "foreground": "#40e0d0"
      }
    },
    {
      "name": "Storage and Types",
      "scope": [
        "storage",
        "storage.type",
        "support.type",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#ffa07a"
      }
    },
    {
      "name": "Functions and Methods",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#dda0dd"
      }
    },
    {
      "name": "Classes and Types",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.class",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#ffa07a"
      }
    },
    {
      "name": "Variables and Properties",
      "scope": [
        "variable",
        "variable.other.readwrite",
        "variable.parameter",
        "meta.object-literal.key",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#ffa07a"
      }
    },
    {
      "name": "Language Variables",
      "scope": [
        "variable.language",
        "variable.parameter.function.language.special"
      ],
      "settings": {
        "foreground": "#ffa07a",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Constants and Numbers",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#5cc9f5"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#40e0d0"
      }
    },
    {
      "name": "String Punctuation",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#40e0d0"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp",
        "constant.other.character-class.regexp",
        "keyword.operator.quantifier.regexp"
      ],
      "settings": {
        "foreground": "#40e0d0"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison"
      ],
      "settings": {
        "foreground": "#40e0d0"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.definition"
      ],
      "settings": {
        "foreground": "#e0e6ed"
      }
    },
    
    // Language-specific enhancements
    {
      "name": "JavaScript/TypeScript Import/Export",
      "scope": [
        "keyword.operator.expression.import",
        "keyword.operator.expression.export",
        "keyword.control.import",
        "keyword.control.export"
      ],
      "settings": {
        "foreground": "#dda0dd"
      }
    },
    {
      "name": "TypeScript/JavaScript Decorators",
      "scope": [
        "meta.decorator",
        "meta.decorator.ts",
        "meta.decorator.tsx",
        "punctuation.decorator"
      ],
      "settings": {
        "foreground": "#dda0dd"
      }
    },
    {
      "name": "JSX/TSX Tags",
      "scope": [
        "entity.name.tag.jsx",
        "entity.name.tag.tsx",
        "punctuation.definition.tag.jsx",
        "punctuation.definition.tag.tsx"
      ],
      "settings": {
        "foreground": "#ffa07a"
      }
    },
    {
      "name": "Python Magic Methods",
      "scope": [
        "support.variable.magic.python"
      ],
      "settings": {
        "foreground": "#ffa07a"
      }
    },
    {
      "name": "Python Function Decorators",
      "scope": [
        "meta.function.decorator.python",
        "support.token.decorator.python"
      ],
      "settings": {
        "foreground": "#dda0dd"
      }
    },
    {
      "name": "Rust Lifetime Annotations",
      "scope": [
        "storage.modifier.lifetime.rust",
        "entity.name.lifetime.rust"
      ],
      "settings": {
        "foreground": "#ffa07a"
      }
    },
    
    // Markup and Documentation
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "entity.name.section.markdown",
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#ffa07a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold",
        "punctuation.definition.bold.markdown"
      ],
      "settings": {
        "foreground": "#5cc9f5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic",
        "punctuation.definition.italic"
      ],
      "settings": {
        "foreground": "#40e0d0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw.markdown",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#40e0d0"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link.markdown",
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#dda0dd"
      }
    },
    
    // HTML/XML
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#ffa07a"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#5cc9f5"
      }
    },
    {
      "name": "HTML Attribute IDs",
      "scope": [
        "entity.other.attribute-name.id"
      ],
      "settings": {
        "foreground": "#dda0dd"
      }
    },
    
    // CSS/SCSS/LESS
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.scss"
      ],
      "settings": {
        "foreground": "#40e0d0"
      }
    },
    {
      "name": "CSS Values",
      "scope": [
        "support.constant.property-value.css",
        "support.constant.property-value.scss"
      ],
      "settings": {
        "foreground": "#5cc9f5"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "meta.selector.css",
        "entity.other.attribute-name.class.css"
      ],
      "settings": {
        "foreground": "#ffa07a"
      }
    },
    
    // JSON
    {
      "name": "JSON Property Names",
      "scope": [
        "support.type.property-name.json",
        "string.quoted.json"
      ],
      "settings": {
        "foreground": "#ffa07a"
      }
    },
    {
      "name": "JSON Values",
      "scope": [
        "string.quoted.json",
        "constant.language.json"
      ],
      "settings": {
        "foreground": "#40e0d0"
      }
    },
    
    // Error states
    {
      "name": "Invalid/Illegal",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "strikethrough"
      }
    }
  ],
  
